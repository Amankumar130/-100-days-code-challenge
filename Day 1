class Solution{
    public:
    string longest(string names[], int n)
    {
        
        int MAX=0;
        string ans="";
        for(int i=0;i<n;i++)
         {
             
             if(names[i].length()>MAX)
             {
                 MAX=names[i].length();
                 ans=names[i];
             }
         }
         return ans;
    }
};

